{
	"name": "url-shorten",
	"author": "Oliver Fleckenstein",
	"homepage": "https://github.com/OliverFlecke/url-shortener",
	"main": "server/index.js",
	"license": "MIT",
	"private": false,
	"scripts": {
		"test": "jest",
		"lint": "yarn run eslint . --fix && yarn prettier . --write",
		"css": "cross-env TAILWIND_MODE=watch yarn postcss src/main.css -o src/compiled.css -v --watch",
		"dev:js": "nodemon",
		"dev": "concurrently --kill-others --names \"JS,CSS\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn dev:js\" \"yarn css\"",
		"build:css": "yarn postcss src/main.css -o src/compiled.css -v",
		"build": "yarn build:css && next build && tsc --project tsconfig.server.json",
		"start": "cross-env NODE_ENV=production node dist/server/index.js",
		"prepare": "husky install"
	},
	"dependencies": {
		"cross-env": "^7.0.2",
		"express": "^4.17.2",
		"mongodb": "^4.3.0",
		"next": "latest",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jest": "^27.4.0",
		"@types/mongodb": "^4.0.7",
		"@types/node": "^12.0.12",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"@types/supertest": "^2.0.11",
		"@types/tailwindcss": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.9.0",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"cssnano": "^5.0.15",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^7.0.4",
		"ionicons": "^6.0.0",
		"jest": "^27.4.7",
		"lint-staged": "^12.1.7",
		"nodemon": "^2.0.4",
		"postcss": "^8.4.5",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.0.2",
		"prettier": "^2.5.1",
		"supertest": "^6.1.6",
		"tailwindcss": "^3.0.12",
		"ts-node": "^8.10.2",
		"typescript": "4.5.4"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"jest": {
		"testEnvironment": "node"
	}
}
